package chip.devicecontroller.ClusterIDMapping

object Clusters {
  fun getCluster(clusterId: Long): BaseCluster? {
    {%- for cluster in clientClusters | sort(attribute='code') %}
    if (clusterId == {{cluster.name}}.ID) {
      return {{cluster.name}}
    }
    {%- endfor %}
    return null
  }
}